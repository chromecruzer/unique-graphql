  "license": "ISC",
  "dependencies": {
    "@babel/core": "^7.24.9",
    "@babel/preset-env": "^7.24.8",
    "@graphql-tools/load-files": "^7.0.0",
    "ajv": "^8.17.1",
    "ajv-keywords": "^5.1.0",
    "babel-loader": "^9.1.3",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "^16.9.0",
    "graphql-subscriptions": "^2.0.0",
    "graphql-tools": "^9.0.1",
    "graphql-ws": "^5.16.0",
    "graphql-yoga": "^5.5.0",
    "morgan": "^1.10.0",
    "terser": "^5.31.1",
    "terser-webpack-plugin": "^5.3.10",
